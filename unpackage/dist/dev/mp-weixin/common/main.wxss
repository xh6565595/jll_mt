/* 字号 */
.cm_t_20 {
	font-size: 20rpx;
}
.cm_t_24 {
	font-size: 24rpx;
}
.cm_t_32 {
	font-size: 32rpx;
}
.cm_tex_c {
	text-align: center;
}
.cm_tex_l {
	text-align: left;
}
.cm_tex_r {
	text-align: right;
}
.cover {
	width: 100%;
	height: 100%;
}
.cm_ellipsis {
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}
.cm_ellipsis2 {
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
.cm_bdb {
	position: relative;
}
.cm_bdb::before {
	content: '';
	width: 100%;
	height: 1rpx;
	background: #f3f3f3;
	-webkit-transform: scaleY(.5);
	        transform: scaleY(.5);
	position: absolute;
	bottom: 0;
	left: 0;
}
.cm_bdt {
	position: relative;
}
.cm_bdt::before {
	content: '';
	width: 100%;
	height: 1rpx;
	background: #f3f3f3;
	-webkit-transform: scaleY(.5);
	        transform: scaleY(.5);
	position: absolute;
	top: 0;
	left: 0;
}
